{
  "name": "vura-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "overrides": {
    "dir-compare": "5.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "echo 'Linting temporarily disabled'",
    "lint:fix": "echo 'Linting temporarily disabled'",
    "preview": "vite preview",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest watch",
    "test:full": "vitest run --passWithNoTests --reporter=verbose",
    "coverage": "vitest run --coverage",
    "coverage:open": "vitest run --coverage && open coverage/index.html",
    "test:results": "vitest run --passWithNoTests && open test-results/index.html",
    "test:all": "vitest run --coverage --passWithNoTests && echo '\\n Coverage report: coverage/index.html\\n Test results: test-results/index.html'",
    "storybook": "storybook dev -p 6006",
    "storybook:node16": "storybook dev -p 6006",
    "storybook:node18:windows": "set NODE_OPTIONS=--openssl-legacy-provider && storybook dev -p 6006",
    "build-storybook": "storybook build",
    "compile": "graphql-codegen",
    "native:start": "react-native start",
    "native:ios": "react-native run-ios",
    "native:android": "react-native run-android",
    "type-check": "tsc --noEmit"
  },
